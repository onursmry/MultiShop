@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_PartialAdminLayoutHeadComponent")
<body class="sidebar-mini">
    <div class="wrapper">

        <!-- Main Header -->
        @await Component.InvokeAsync("_PartialAdminLayoutHeaderComponent")
        <!-- Left side column. contains the logo and sidebar -->
        @await Component.InvokeAsync("_PartialAdminLayoutSidebarComponent")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            @await Component.InvokeAsync("_PartialAdminLayoutMainSectionViewBagComponent")

            <!-- Main content -->
            @RenderBody()
            <!-- content -->
        </div>
        <!-- content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer dark-bg">
            <div class="pull-right hidden-xs"> Version 1.0</div>
            Copyright &copy; @DateTime.Now.Year MultiShop. All rights reserved.
        </footer>
    </div>
    <!-- wrapper -->
    <!-- jQuery -->
    <script src="~/MultiShopAdmin/html/dist/js/jquery.min.js"></script>
    <script src="~/MultiShopAdmin/html/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/MultiShopAdmin/html/dist/js/ovio.js"></script>
</body>
</html>